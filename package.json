{
  "name": "@coprocure/search-widget",
  "version": "2.1.13",
  "description": "CoProcure search widget",
  "main": "src/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "karma start --coverage",
    "build": "webpack --progress --colors",
    "watch": "webpack --progress --colors --watch",
    "start:dev": "webpack-dev-server --open",
    "prepublishOnly": "npm version patch && npm run build",
    "getup": "npm publish --access public",
    "postpublish": "git push origin --all; git push origin --tags",
    "postversion": "npm publish",
    "preversion": "npm test",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "test:legacy": "karma start --legacy --coverage",
    "test:legacy:watch": "karma start --legacy --auto-watch=true --single-run=false",
    "test:bs": "karma start karma.bs.config.js --legacy --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CoProcure/coprocure-search-widget"
  },
  "devDependencies": {
    "@open-wc/testing": "^0.11.1",
    "@open-wc/testing-karma": "^1.0.0",
    "@open-wc/testing-karma-bs": "^1.0.0",
    "chai": "^4.2.0",
    "css-loader": "^1.0.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^3.2.0",
    "lit-html": "^1.0.0",
    "mocha": "^6.1.3",
    "node-sass": "^4.12.0",
    "sass-loader": "^6.0.7",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "style-loader": "^0.20.3",
    "to-string-loader": "^1.1.5",
    "webdriverio": "^5.6.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5"
  },
  "keywords": [
    "web component"
  ],
  "license": "ISC",
  "dependencies": {}
}
